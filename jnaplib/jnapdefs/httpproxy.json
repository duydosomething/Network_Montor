{
    "name": "http://linksys.com/jnap/httpproxy/HttpProxy", 
    "desc": "This service provides managing http proxy rules on the router.", 
    "actions": {
        "AddHttpProxyRule": {
            "name": "http://linksys.com/jnap/httpproxy/AddHttpProxyRule", 
            "desc": "This action adds a new HTTP Proxy rule.", 
            "input": {
                "headerToken": {
                    "type": "string", 
                    "array": false, 
                    "optional": true, 
                    "desc": "Http header token."
                }, 
                "passPhrase": {
                    "type": "string", 
                    "array": false, 
                    "optional": true, 
                    "desc": "Pass phrase for URL encryption."
                }, 
                "useSSL": {
                    "type": "bool", 
                    "array": false, 
                    "optional": false, 
                    "desc": "Use SSL for encryption or not."
                }, 
                "acls": {
                    "type": "HttpACL", 
                    "array": true, 
                    "optional": false, 
                    "desc": "An array of access control list."
                }, 
                "sessionUUID": {
                    "type": "UUID", 
                    "array": false, 
                    "optional": true, 
                    "desc": "Session UUID associated with the http proxy rule"
                }
            }, 
            "output": {
                "ruleUUID": {
                    "type": "UUID", 
                    "array": false, 
                    "optional": false, 
                    "desc": "rule UUID identifies a rule."
                }
            }, 
            "result": {
                "errors": {
                    "Error": "", 
                    "ErrorInvalidACLType": "The ACLType could only be Allow or Deny.", 
                    "ErrorInvalidDestPort": "The port should be between 0 and 65535.", 
                    "ErrorInvalidIpv4PrefixLength": "The ipv4PrefixLength should be at the range of [0, 32].", 
                    "ErrorInvalidIpv6PrefixLength": "The ipv6PrefixLength should be at the range of [0, 64].", 
                    "ErrorSessionDoesNotExist": "The specified session does not exit.", 
                    "ErrorTooManyAclsPerHttpProxyRule": "MAX is 10.", 
                    "ErrorTooManyHttpProxyRules": "MAX is 20.", 
                    "_ErrorAbortedAction": "", 
                    "_ErrorInvalidInput": "", 
                    "_ErrorInvalidOutput": "", 
                    "_ErrorNotReady": "", 
                    "_ErrorSessionVerification": "", 
                    "_ErrorUnauthorized": "", 
                    "_ErrorUnexpected": "", 
                    "_ErrorUnknownAction": "", 
                    "_ErrorUnknownSession": ""
                }, 
                "success": {
                    "OK": "Success."
                }
            }
        }, 
        "GetHttpProxyParameters": {
            "name": "http://linksys.com/jnap/httpproxy/GetHttpProxyParameters", 
            "desc": "This action gets http proxy parameters.", 
            "output": {
                "httpPort": {
                    "type": "int", 
                    "array": false, 
                    "optional": false, 
                    "desc": "Http port"
                }, 
                "httpsPort": {
                    "type": "int", 
                    "array": false, 
                    "optional": false, 
                    "desc": "Https port"
                }, 
                "maxHttpProxyRules": {
                    "type": "int", 
                    "array": false, 
                    "optional": false, 
                    "desc": "Max number of http proxy rules allowed on the router"
                }, 
                "maxACLRulesPerHttpProxyRule": {
                    "type": "int", 
                    "array": false, 
                    "optional": false, 
                    "desc": "Max number of ACL rules per http proxy rule allowed on the router"
                }
            }, 
            "result": {
                "errors": {
                    "Error": "", 
                    "_ErrorAbortedAction": "", 
                    "_ErrorInvalidInput": "", 
                    "_ErrorInvalidOutput": "", 
                    "_ErrorNotReady": "", 
                    "_ErrorSessionVerification": "", 
                    "_ErrorUnauthorized": "", 
                    "_ErrorUnexpected": "", 
                    "_ErrorUnknownAction": "", 
                    "_ErrorUnknownSession": ""
                }, 
                "success": {
                    "OK": "Success."
                }
            }
        }, 
        "RemoveHttpProxyRule": {
            "name": "http://linksys.com/jnap/httpproxy/RemoveHttpProxyRule", 
            "desc": "This action removes HTTP Proxy rule.", 
            "input": {
                "ruleUUID": {
                    "type": "UUID", 
                    "array": false, 
                    "optional": false, 
                    "desc": "Rule UUID identifies a rule."
                }
            }, 
            "result": {
                "errors": {
                    "Error": "", 
                    "ErrorHttpProxyRuleDoesNotExist": "The rule does not exist.", 
                    "_ErrorAbortedAction": "", 
                    "_ErrorInvalidInput": "", 
                    "_ErrorInvalidOutput": "", 
                    "_ErrorNotReady": "", 
                    "_ErrorSessionVerification": "", 
                    "_ErrorUnauthorized": "", 
                    "_ErrorUnexpected": "", 
                    "_ErrorUnknownAction": "", 
                    "_ErrorUnknownSession": ""
                }, 
                "success": {
                    "OK": "Success."
                }
            }
        }
    }, 
    "structs": {
        "HttpACL": {
            "aclType": {
                "type": "ACLType", 
                "array": false, 
                "optional": false, 
                "desc": "ACL type."
            }, 
            "ipv4Address": {
                "type": "IPAddress", 
                "array": false, 
                "optional": true, 
                "desc": "Source IPv4 address"
            }, 
            "ipv4PrefixLength": {
                "type": "int", 
                "array": false, 
                "optional": true, 
                "desc": "The IPv4 prefix length. This value must be between 0 and 32."
            }, 
            "ipv6Address": {
                "type": "IPv6Address", 
                "array": false, 
                "optional": true, 
                "desc": "Source IPv6 address"
            }, 
            "ipv6PrefixLength": {
                "type": "int", 
                "array": false, 
                "optional": true, 
                "desc": "The IPv6 prefix length. This value must be between 0 and 64."
            }, 
            "destPort": {
                "type": "int", 
                "array": false, 
                "optional": true, 
                "desc": "Destination port of rule"
            }
        }
    }, 
    "enums": {
        "ACLType": {
            "Allow": "Allow.", 
            "Deny": "Deny."
        }
    }
}