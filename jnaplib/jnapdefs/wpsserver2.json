{
    "name": "http://linksys.com/jnap/wirelessap/WPSServer2", 
    "desc": "This service extends the WPSServer service, allowing a client to provision the WPS server settings on the wireless access point.", 
    "actions": {
        "GetWPSServerSessionStatus": {
            "name": "http://linksys.com/jnap/wirelessap/GetWPSServerSessionStatus", 
            "desc": "This action gets the status of the WPS session, if any, that is currently in progress on the wireless access point.", 
            "output": {
                "isWPSSessionInProgress": {
                    "type": "bool", 
                    "array": false, 
                    "optional": false, 
                    "desc": "Whether a WPS session is currently in progress."
                }, 
                "serverPIN": {
                    "type": "string", 
                    "array": false, 
                    "optional": false, 
                    "desc": "The WPS server's PIN number."
                }, 
                "lastResult": {
                    "type": "WPSSessionResult", 
                    "array": false, 
                    "optional": true, 
                    "desc": "The result of the last WPS session, if present."
                }
            }, 
            "result": {
                "errors": {
                    "Error": "", 
                    "_ErrorAbortedAction": "", 
                    "_ErrorInvalidInput": "", 
                    "_ErrorInvalidOutput": "", 
                    "_ErrorNotReady": "", 
                    "_ErrorSessionVerification": "", 
                    "_ErrorUnauthorized": "", 
                    "_ErrorUnexpected": "", 
                    "_ErrorUnknownAction": "", 
                    "_ErrorUnknownSession": ""
                }, 
                "success": {
                    "OK": "Success."
                }
            }
        }, 
        "GetWPSServerSettings": {
            "name": "http://linksys.com/jnap/wirelessap/GetWPSServerSettings", 
            "desc": "", 
            "output": {
                "enabled": {
                    "type": "bool", 
                    "array": false, 
                    "optional": false, 
                    "desc": "Whether the WPS server is enabled."
                }
            }, 
            "result": {
                "errors": {
                    "Error": "", 
                    "_ErrorAbortedAction": "", 
                    "_ErrorInvalidInput": "", 
                    "_ErrorInvalidOutput": "", 
                    "_ErrorNotReady": "", 
                    "_ErrorSessionVerification": "", 
                    "_ErrorUnauthorized": "", 
                    "_ErrorUnexpected": "", 
                    "_ErrorUnknownAction": "", 
                    "_ErrorUnknownSession": ""
                }, 
                "success": {
                    "OK": "Success."
                }
            }
        }, 
        "SetWPSServerSettings": {
            "name": "http://linksys.com/jnap/wirelessap/SetWPSServerSettings", 
            "desc": "", 
            "input": {
                "enabled": {
                    "type": "bool", 
                    "array": false, 
                    "optional": false, 
                    "desc": "Whether to enable the WPS server."
                }
            }, 
            "result": {
                "errors": {
                    "Error": "", 
                    "_ErrorAbortedAction": "", 
                    "_ErrorInvalidInput": "", 
                    "_ErrorInvalidOutput": "", 
                    "_ErrorNotReady": "", 
                    "_ErrorSessionVerification": "", 
                    "_ErrorUnauthorized": "", 
                    "_ErrorUnexpected": "", 
                    "_ErrorUnknownAction": "", 
                    "_ErrorUnknownSession": ""
                }, 
                "success": {
                    "OK": "Success."
                }
            }
        }, 
        "StartWPSServerSession": {
            "name": "http://linksys.com/jnap/wirelessap/StartWPSServerSession", 
            "desc": "This action starts a WPS session on the wireless access point.", 
            "input": {
                "clientPIN": {
                    "type": "string", 
                    "array": false, 
                    "optional": true, 
                    "desc": "The client-supplied PIN, if any, to use for the WPS session. If this value is not present, the WPS session will be in pushbutton mode and will not use a client PIN. If this value is present, it must be a valid 4 or 8 digit WPS PIN. 8-digit PINs will be verified using the checksum algorithm defined in the WPS specification."
                }
            }, 
            "result": {
                "errors": {
                    "Error": "", 
                    "ErrorInvalidClientPIN": "The specified client PIN is not valid.", 
                    "ErrorWPSServerNotEnabled": "The WPS server is not enabled.", 
                    "ErrorWPSSessionAlreadyInProgress": "A WPS session is already in progress.", 
                    "_ErrorAbortedAction": "", 
                    "_ErrorInvalidInput": "", 
                    "_ErrorInvalidOutput": "", 
                    "_ErrorNotReady": "", 
                    "_ErrorSessionVerification": "", 
                    "_ErrorUnauthorized": "", 
                    "_ErrorUnexpected": "", 
                    "_ErrorUnknownAction": "", 
                    "_ErrorUnknownSession": ""
                }, 
                "success": {
                    "OK": "Success."
                }
            }
        }, 
        "StopWPSServerSession": {
            "name": "http://linksys.com/jnap/wirelessap/StopWPSServerSession", 
            "desc": "This action stops the current WPS session on the wireless access point.", 
            "result": {
                "errors": {
                    "Error": "", 
                    "ErrorWPSSessionNotInProgress": "No WPS session is currently in progress.", 
                    "_ErrorAbortedAction": "", 
                    "_ErrorInvalidInput": "", 
                    "_ErrorInvalidOutput": "", 
                    "_ErrorNotReady": "", 
                    "_ErrorSessionVerification": "", 
                    "_ErrorUnauthorized": "", 
                    "_ErrorUnexpected": "", 
                    "_ErrorUnknownAction": "", 
                    "_ErrorUnknownSession": ""
                }, 
                "success": {
                    "OK": "Success."
                }
            }
        }
    }, 
    "enums": {
        "WPSSessionResult": {
            "Failed": "The last WPS session failed.", 
            "Succeeded": "The last WPS session succeeded."
        }
    }
}